    <record id="view_company_form_city_zip" model="ir.ui.view">
        <field name="name">res.company.form.city.zip</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base_location.view_company_form_city"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='zip_id']" position="replace">
                <field name="zip_id" class="oe_edit_only"/>
            </xpath>
        </field>
