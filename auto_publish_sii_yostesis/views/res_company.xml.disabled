<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_company_sii_start_form" model="ir.ui.view">
        <field name="name">res.company.sii.start.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="l10n_es_aeat_sii_oca.view_company_sii_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sii_period']" position="after">
                <field name="sii_date_start"/>
            </xpath>
            <xpath expr="//field[@name='sii_test']" position="before">
                <field name="sii_exonerated"/>
                <field name="sii_auto_upload"/>
            </xpath>
        </field>
    </record>

